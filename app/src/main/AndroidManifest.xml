<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.odairtns.trackyourexpenses.Activities.InsertRecord"></activity>
        <activity android:name="com.odairtns.trackyourexpenses.Activities.NewTrip" />
        <activity android:name="com.odairtns.trackyourexpenses.Activities.SearchTrip" />
        <activity android:name="com.odairtns.trackyourexpenses.Activities.TripChart" />
        <activity android:name="com.odairtns.trackyourexpenses.Activities.ViewTripRecord" />
        <activity
            android:name="com.odairtns.trackyourexpenses.Activities.TripCurrencies"
            android:label="@string/title_activity_trip_currencies"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.odairtns.trackyourexpenses.Activities.ExpenseType"
            android:label="@string/title_activity_expense_type"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.odairtns.trackyourexpenses.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>